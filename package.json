{
  "name": "givenNames",
  "version": "0.2.0",
  "description": "Herramientas para rastreo e importaciÃ³n de nombres propios (crawler Playwright + importador SQLite v2).",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "typecheck": "tsc --noEmit",
    "playwright:install": "playwright install chromium",
    "crawl": "ts-node crawler.ts --seeds seeds.txt --out out.csv --depth 2 --concurrency 3 --rate-limit-ms 500 --max-pages 500 --same-domain-only true --respect-robots true --respect-nofollow true --pause-on-captcha true",
    "crawl:unrestricted": "ts-node crawler.ts --seeds seeds.txt --out out_unrestricted.csv --depth 3 --concurrency 5 --rate-limit-ms 0 --max-pages 0 --same-domain-only false --respect-robots false --respect-nofollow false --pause-on-captcha false",
    "import": "ts-node importer.ts --db ./nombres.sqlite --in ./f-xx_xx-nombres.txt",
    "test": "jest --verbose --runInBand",
    "test:vitest": "vitest run",
    "test:vitest:ui": "vitest --ui",
    "check:unused": "ts-node scripts/findUnusedFunctions.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.3",
    "argon2": "^0.44.0",
    "better-sqlite3": "^9.4.0",
    "dotenv": "^17.2.2",
    "fast-glob": "^3.3.3",
    "hono": "^4.9.8",
    "http-terminator": "^3.2.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "minimist": "^1.2.8",
    "playwright": "^1.47.0",
    "ts-morph": "^27.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "ts-node": {
    "compilerOptions": {
      "target": "ES2021",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "esm": true,
    "transpileOnly": true,
    "expermentalSpecificationResolution": "node"
  }
}
